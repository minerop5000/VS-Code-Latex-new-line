{
	"name": "latex-new-line",
	"displayName": "Latex new line",
	"description": "Insert double backslash for new line with shift+enter",
	"publisher": "Minerop",
	"version": "0.3.1",
	"icon": "images/logo.png",
	"repository": {
		"url": "https://github.com/minerop5000/VS-Code-Latex-new-line"
	},
	"engines": {
		"vscode": "^1.4.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:latex-new-line.new-line"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "latex-new-line.new-line",
				"title": "Latex new line"
			}
		],
		"keybindings": [
			{
				"command": "latex-new-line.new-line",
				"key": "shift+enter",
				"mac": "shift+enter"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"deploy": "vsce publish --yarn"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.4.0",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.4",
		"@vscode/vsce": "^2.19.0"
	}
}
